{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "edge": "17",
                    "firefox": "60",
                    "chrome": "67",
                    "safari": "11.1",
                    "node": "current"
                },
                "useBuiltIns": "usage",
                "corejs": "3.6.5"
            }
        ],
        [
            "@babel/preset-react",
            {
                "targets": {
                    "node": "current"
                },
                "pragma": "dom",
                "pragmaFrag": "DomFrag",
                "throwIfNamespace": false,
                "runtime": "classic"
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-syntax-jsx",
        "@babel/plugin-transform-modules-commonjs",
        [
            "@babel/plugin-transform-react-jsx",
            {
                "throwIfNamespace": false,
                "runtime": "automatic"
            }
        ]
    ]
}